{
    "env_name": "Pendulum-v1",
    "behavior_policy_path": "opelab/examples/gym/policy/pendulum/Pi_3.pkl",
    "dataset_path":"opelab/examples/gym/dataset/pdataset/",
    "guidance_hyperparams": {
        "action_scale": 1.0,
        "state_scale": 0.0,
        "use_adaptive": false,
        "use_neg_grad": true,
        "neg_grad_scale": 0.1,
        "normalize_grad": true,
        "k_guide": 1,
        "use_action_grad_only": true,
        "clamp" : false,
        "l_inf" : 10,
        "ratio" : 1.0
    },
    "target_policy_paths": [
        "opelab/examples/gym/policy/pendulum/Pi_1.pkl",
        "opelab/examples/gym/policy/pendulum/Pi_2.pkl",
        "opelab/examples/gym/policy/pendulum/Pi_3.pkl",
        "opelab/examples/gym/policy/pendulum/Pi_4.pkl",
        "opelab/examples/gym/policy/pendulum/Pi_5.pkl"
        ],
    "baseline_configs": {
        "FQE":{
                    "class": "FQE_JAX",
                    "params":{
                        "lr": 0.003,
                        "tau": 0.005,
                        "layers": [256, 256],
                        "epochs": 100,
                        "batch_size": 128
                    }
                }
    },
    "experiment_params": {
        "horizon": 196,
        "rollouts": 50,
        "gamma": 0.99,
        "trials": 1,
        "save_path": "results/pendulum/fqe",
        "top_k": 2,
        "oracle_rollouts": 10
    }
}
